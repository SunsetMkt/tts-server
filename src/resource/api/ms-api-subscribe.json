{
  "api_name": "ms-tts-subscribe",
  "api_desc": "微软语音合成 - 订阅API接口",
  "api_url": "/api/tts-ms-subscribe-api",
  "params": [
    {
      "index": 0,
      "param_type": "Text",
      "param_name": "text",
      "param_desc": "待生成文本",
      "max_len": 5000
    },
    {
      "index": 1,
      "param_type": "List",
      "param_name": "informant",
      "param_desc": "发音人",
      "list_data_url": "/api/ms-tts/informant/{{api_name}}"
    },
    {
      "index": 2,
      "param_type": "List",
      "param_name": "style",
      "param_desc": "音频风格",
      "list_data_url": "/api/ms-tts/style/{{informant}}"
    },
    {
      "index": 3,
      "param_type": "Float",
      "param_name": "rate",
      "param_desc": "语速",
      "min_value": 0.0,
      "max_value": 3.0,
      "default_value": 1.0
    },
    {
      "index": 4,
      "param_type": "Float",
      "param_name": "pitch",
      "param_desc": "音调",
      "min_value": 0.0,
      "max_value": 2.0,
      "default_value": 1.0
    },
    {
      "index": 5,
      "param_type": "List",
      "param_name": "quality",
      "param_desc": "音频格式",
      "list_data_url": "/api/ms-tts/quality"
    }
  ]
}